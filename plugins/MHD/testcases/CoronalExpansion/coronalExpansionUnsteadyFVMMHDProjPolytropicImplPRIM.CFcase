Simulator.SubSystem.LinearSystemSolver = PETSC
Simulator.SubSystem.LSSNames = CrankNicholsonLSS
Simulator.SubSystem.CrankNicholsonLSS.Data.PCType = PCASM
Simulator.SubSystem.CrankNicholsonLSS.Data.KSPType = KSPGMRES
Simulator.SubSystem.CrankNicholsonLSS.Data.MatOrderingType = MATORDERING_RCM
Simulator.SubSystem.CrankNicholsonLSS.Data.RelativeTolerance = 1e-4

Simulator.SubSystem.ConvergenceMethod = BDF2
Simulator.SubSystem.BDF2.ShowRate = 1
#Simulator.SubSystem.BDF2.Data.CFL.Value = 1.0
Simulator.SubSystem.BDF2.ConvergenceFile = convergence_coronalExpansionUnsteadyFVMMHDProjPolytropicImpl.plt
Simulator.SubSystem.BDF2.Data.CFL.ComputeCFL = Interactive
