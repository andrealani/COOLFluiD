Simulator.SubSystem.InteractiveParamReader.FileName = plugins/MHD/testcases/CoronalExpansion/coronalExpansion.inter

Simulator.SubSystem.OutputFormat        = Tecplot CFmesh
Simulator.SubSystem.CellCenterFVM.Restart = true
Simulator.SubSystem.CFmesh.FileName     = coronalExpansionUnsteadyFVMMHDProjPolytropicImpl.CFmesh
Simulator.SubSystem.Tecplot.FileName    = coronalExpansionUnsteadyFVMMHDProjPolytropicImpl.plt
Simulator.SubSystem.Tecplot.Data.printExtraValues = true
