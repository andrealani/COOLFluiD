#######TEST CONFIGURATION
CFEnv.ErrorOnUnusedConfig = true

# Simulation Modules
Simulator.Modules.Libs = libPetscI libTecplotWriter libNavierStokes libMutation2OLD libMutation2OLDI libMutationppI libLTE libICP libFiniteVolume libFiniteVolumeNavierStokes libFiniteVolumeICP libNewtonMethod libGambit2CFmesh libCFmeshFileReader libCFmeshFileWriter

# Simulation Parameters
Simulator.Paths.WorkingDir = plugins/ICP/testcases/Plasmatron/Chamber
Simulator.Paths.ResultsDir = ./OUTPUT_chamber

###### ICP PHYSICAL MODEL ##
Simulator.SubSystem.Default.PhysicalModelType = ICPLTE2D
