Simulator.SubSystem.ICPLTE2D.refLength = 1.

Simulator.SubSystem.ICPLTE2D.ConvTerm.uInf = 666.
Simulator.SubSystem.ICPLTE2D.ConvTerm.p0Inf = 1500.

###### EM Field MODEL ##

Simulator.SubSystem.ICPLTE2D.SourceTerm.Use2DModel = true
